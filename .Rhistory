library(base64enc)
# Read the PNG file
png_path <- "AMES.png"
png_data <- base64encode(readBin(png_path, "raw", file.size(png_path)))
# Create an HTML document with the embedded image
html_code <- sprintf('<html><body><img src="data:image/png;base64,%s"></body></html>', png_data)
# Write the HTML code to a file
html_file <- "path/to/your/output.html"
writeLines(html_code, html_file
